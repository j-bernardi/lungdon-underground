 {
     "version": 2,

     "builds": [
         {
             "src": "app_files/app.py",
             "use": "@vercel/python"
         }
     ],
     "routes": [
        {
            "src": "/static/(.*)",
            "dest": "app_files/static/$1"
        },
        {
             "src": "/(.*)",
             "dest": "app_files/app.py"
         }
     ]
 }
