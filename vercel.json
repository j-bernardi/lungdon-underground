 {
    "version": 2,
     "builds": [
         {
             "src": "api/app.py",
             "use": "@vercel/python"
         },
         {
            "src": "api/static/**",
            "use": "@vercel/static"
          }
     ],
     "rewrites": [
        {
            "source": "/static/(.*)",
            "destination": "/api/static/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/api/app.py" 
        }
    ]
 }
